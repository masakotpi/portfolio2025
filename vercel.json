{
  "regions": [
      "hnd1"
  ],
  "builds": [
      {
          "src": "/api/index.php",
          "use": "vercel-php@0.7.3"
      },
      {
          "src": "/public/**",
          "use": "@vercel/static"
      }
  ],
  
  "env": {
      "APP_NAME": "Laravel Vercel",
      "APP_DEBUG": "false",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "CACHE_DRIVER": "array",
      "CACHE_STORE": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "VIEW_COMPILED_PATH": "/tmp",
      
      "DB_CONNECTION" : "pgsql",
      "DB_HOST" : "ep-twilight-unit-a1waa9ob-pooler.ap-southeast-1.aws.neon.tech",
      "DB_PORT" : "5432",
      "DB_DATABASE" : "neondb",
      "DB_USERNAME" : "neondb_owner",
      "DB_PASSWORD" : "npg_s1CAzjOcnbH9",
      "DATABASE_URL" : "postgres://neondb_owner:npg_s1CAzjOcnbH9@ep-twilight-unit-a1waa9ob-pooler.ap-southeast-1.aws.neon.tech/neondb?sslmode=require"
      
  },
  "rewrites": [
      { "source": "/", "destination": "/api/index.php" },
      { "source": "/(.*\\.jpg)", "destination": "/public/$1" },
      { "source": "/(.*\\.png)", "destination": "/public/$1" },
      { "source": "/(.*\\.css)", "destination": "/public/$1" },
      { "source": "/(.*\\.js)", "destination": "/public/$1" },
      { "source": "/(.*\\.svg)", "destination": "/public/$1" },
      { "source": "/(.*\\.ico)", "destination": "/public/$1" },
      { "source": "/(.*)", "destination": "/public/$1" },
      { "source": "/(.*)", "destination": "/api/index.php" }
    ]
}
